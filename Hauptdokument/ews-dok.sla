  <PAGEOBJECT OnMasterPage="" BottomLine="0" REXTRA="0" ALIGN="3" gHeight="8.69556e-322" gWidth="2.36942e-310" LANGUAGE="English" NUMPO="16" PLINEART="1" TXTSCALE="100" RightLine="0" LOCALSCX="1" LINESP="12" ROT="0" TXTSHX="5" TXTSTROKE="Black" WIDTH="197.983" ImageRes="1" GROUPS="" LOCKR="0" IFONT="Times New Roman Regular" LOCALSCY="1" NAMEDLST="" TXTSHY="-5" isInline="0" AUTOTEXT="0" FLIPPEDV="0" PCOLOR="None" RADRECT="0" REVERS="0" PRINTABLE="1" RATIO="1" FLIPPEDH="0" COLGAP="0" PCOLOR2="None" NEXTITEM="75" NUMGROUP="0" TransValue="0" PLINEEND="0" TXTSTW="-0.1" FRTYPE="0" PTYPE="4" ImageClip="" isTableItem="0" TEXTFLOW2="0" SHADE2="100" TXTBASE="0" PWIDTH="1" HEIGHT="534.827" DASHOFF="0" PFILE2="" PFILE="" TEXTFLOW3="0" ISIZE="10" PLTSHOW="0" LINESPMode="0" TXTSTYLE="0" CLIPEDIT="0" BACKITEM="70" TransValueS="0" EMBEDDED="1" PFILE3="" ANNAME="" SHADE="100" TXTULP="-0.1" fillRule="1" COCOOR="0 0 0 0 197.983 0 197.983 0 197.983 0 197.983 0 197.983 534.827 197.983 534.827 197.983 534.827 197.983 534.827 0 534.827 0 534.827 0 534.827 0 534.827 0 0 0 0 " BASEOF="0" PICART="1" TXTKERN="0" COLUMNS="1" OwnPage="24" LAYER="0" BOOKMARK="0" gYpos="5.72131e-316" startArrowIndex="0" TopLine="0" LOCK="0" EPROF="" gXpos="1.63042e-322" DASHS="" IRENDER="1" TEXTFLOW="0" TXTFILLSH="100" YPOS="7673.21" ANNOTATION="0" LOCALX="0" GRTYP="0" XPOS="540.624" NUMCO="16" POCOOR="0 0 0 0 197.983 0 197.983 0 197.983 0 197.983 0 197.983 534.827 197.983 534.827 197.983 534.827 197.983 534.827 0 534.827 0 534.827 0 534.827 0 534.827 0 0 0 0 " EXTRA="0" LOCALY="0" NUMDASH="0" LeftLine="0" PRFILE="" TEXTRA="0" SCALETYPE="1" TXTOUT="1" TXTFILL="Black" endArrowIndex="0" BEXTRA="0" PLINEJOIN="0" TXTSTP="-0.1" TXTULW="-0.1" TXTSCALEV="100" TXTSTRSH="100" >
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Wunden davon, heilt er in normaler Umgebung nichts mehr. Entscheidet er sich also, trotz zweier Wunden weiter als Diplomat zu arbeiten, statt sich Ruhe zu gönnen, dann erholt er sich nie. &#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light Italic" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="7" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Heilungsumstände leiten sich aus zwei Fragen her: &quot;Was macht der Charakter?&quot;, und &quot;Wie wird seine Heilung gefördert?&quot; &#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Wenn ein Charakter sich Ruhe gönnt und im Bett bleibt, erhält er einen Punkt Bonus. Wenn er dagegen auf Reisen geht, bekommt er einen Malus von 1 oder mehr. Auf einem Gewalt-marsch steigt dieser Malus bei-spielsweise auf 4, so dass Charak-tere mit einer Wundschwelle unter 6 regelmäßig neue Wunden erhalten. &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Vn Nimbus Mono L Regular" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="7" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="&quot;Grok, Barbar&quot; hat sich bei einem Fürsten verdungen. Als der Fürst sich entscheidet, seinen Nachbarn für eine Schmähung beim Ball anzugreifen, schickt er Groks Einheit auf einen mehrwöchentlichen Gewaltmarsch. Grok mit seiner überragenden Konstitution (18, WS 6) hält den Marsch gut durch (6/(6-2-4) = 6/0 = keine Heilung), doch die anderen in seiner Einheit (WS 4) erhalten alle 2 Wochen eine Wunde (4 /(4-2-4) = -2). Als Grok nach 6 Wochen alleine ankommt, entscheidet er sich, in der Schenke nach schöneren Aufgaben zu suchen. &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light Italic" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="7" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Fachkundliche Behandlung gibt Boni auf die Heilung. Einfache Kräuter-kunde gibt einen Bonus von 1, ein modernes Krankenhaus von 2, und Science Fiction Kliniken können Boni von 6 und mehr geben. Bleiben Wunden dagegen völlig unbehandelt, kann das Mali von -1 bis -3 nach sich ziehen; zusätzlich zu dem Malus von 1 pro Wunde. &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Vn Nimbus Mono L Regular" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="7" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="&#x5;Selbst Grok sollte also seine Wunden zumindest verbinden. &#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="8" CULP="-0.1" CH="Das Fokusmodul Kampf (FoKa) bietet ein komplexeres (aber vollständig kompatibles) Schadens- und Hei-lungs-system, in dem auch Kratzer relevant sind. &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Vn URW Palladio L Roman" CSHX="5" CSHY="-5" CSTROKE="Black" />
