  <PAGEOBJECT OnMasterPage="" BottomLine="0" REXTRA="0" ALIGN="3" gHeight="3.88924e-316" gWidth="4.71774e-316" LANGUAGE="English" NUMPO="16" PLINEART="1" TXTSCALE="100" RightLine="0" LOCALSCX="1" LINESP="12" ROT="0" TXTSHX="5" TXTSTROKE="Black" WIDTH="197.983" ImageRes="1" GROUPS="" LOCKR="0" IFONT="Times New Roman Regular" LOCALSCY="1" NAMEDLST="" TXTSHY="-5" isInline="0" AUTOTEXT="0" FLIPPEDV="0" PCOLOR="None" RADRECT="0" REVERS="0" PRINTABLE="1" RATIO="1" FLIPPEDH="0" COLGAP="0" PCOLOR2="None" NEXTITEM="99" NUMGROUP="0" TransValue="0" PLINEEND="0" TXTSTW="-0.1" FRTYPE="0" PTYPE="4" ImageClip="" isTableItem="0" TEXTFLOW2="0" SHADE2="100" TXTBASE="0" PWIDTH="1" HEIGHT="534.827" DASHOFF="0" PFILE2="" PFILE="" TEXTFLOW3="0" ISIZE="10" PLTSHOW="0" LINESPMode="0" TXTSTYLE="0" CLIPEDIT="0" BACKITEM="94" TransValueS="0" EMBEDDED="1" PFILE3="" ANNAME="" SHADE="100" TXTULP="-0.1" fillRule="1" COCOOR="0 0 0 0 197.983 0 197.983 0 197.983 0 197.983 0 197.983 534.827 197.983 534.827 197.983 534.827 197.983 534.827 0 534.827 0 534.827 0 534.827 0 534.827 0 0 0 0 " BASEOF="0" PICART="1" TXTKERN="0" COLUMNS="1" OwnPage="37" LAYER="0" BOOKMARK="0" gYpos="7.85367e-317" startArrowIndex="0" TopLine="0" LOCK="0" EPROF="" gXpos="0" DASHS="" IRENDER="1" TEXTFLOW="0" TXTFILLSH="100" YPOS="12120.1" ANNOTATION="0" LOCALX="0" GRTYP="0" XPOS="288.341" NUMCO="16" POCOOR="0 0 0 0 197.983 0 197.983 0 197.983 0 197.983 0 197.983 534.827 197.983 534.827 197.983 534.827 197.983 534.827 0 534.827 0 534.827 0 534.827 0 534.827 0 0 0 0 " EXTRA="0" LOCALY="0" NUMDASH="0" LeftLine="0" PRFILE="" TEXTRA="0" SCALETYPE="1" TXTOUT="1" TXTFILL="Black" endArrowIndex="0" BEXTRA="0" PLINEJOIN="0" TXTSTP="-0.1" TXTULW="-0.1" TXTSCALEV="100" TXTSTRSH="100" >
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Knacken von Beton, der nach dem Verschwinden der Sonne langsam abkühlt. &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light Italic" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="7" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Einige lautlose Schritte bringen Stahlkatze zur Brüstung der Wand und dort auf eine verrostete Feuerleiter. Auf halbem Weg sieht er einige Ratten, die sich in nichts von dem Kadaver eines verhungerten Hundes abbringen lassen, ihrem Essen in dieser Nacht. &#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Dann berühren Stahlkatzes Krallen den erkalteten Beton des Gehsteigs unter den riesenhaften Silhouetten der Hochhäuser. Im gleichen Moment zerreißt ein Grollen die Stille, und Stahlkatzes Blick springt die Straße herunter, zur Quelle des Lärms. In der leeren Gasse steht ein monströses stachelbewehrtes Motorrad. &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light Italic" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="7" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Stahlkatze erstarrt für Momente. Der Fahrer reißt eine Stachelkeule vom Boden, tritt das Motorrad los und jagt mit heulendem Motor auf den chromglänzenden Katzenmenschen zu. &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light Italic" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="7" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Während die Silhouette des Motorrades immer weiter wächst und das Grollen des Motors selbst das Innere der Hochhäuser zu erreichen scheint, tritt Stahlkatze einen unmerklichen Schritt zur Seite. Seine Krallen streichen über den polierten Stahl eines Wurfsterns und entlocken ihm ein zartes, stählernes Singen. Sein Blick fixiert seinen Gegner über die Stacheln des Lenkrades hinweg. &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light Italic" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="7" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Als ihn das Motorrad fast erreicht hat, bringen Stahlkatze zwei schnelle Schritte aus der Reichweite der Keule, und die Wurfscheibe verlässt seine Krallen auf einer tödlichen Bahn. &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light Italic" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="7" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="URW Bookman L Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Technisches: &#x5;- Grund-MW: Kurz im Kampf: MW 12&#x5;- Angreifer bewegt sich schnell: MW+3&#x5;- Ziel in 1/3 Deckung (Beine hinter dem Motorrad: MW+3)&#x5;(d.h. der MW ist jetzt 18)&#x5;- Fernkampfwert von Stahlkatze: 12. &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Vn Nimbus Mono L Regular" CSHX="5" CSHY="-5" CSTROKE="Black" />
