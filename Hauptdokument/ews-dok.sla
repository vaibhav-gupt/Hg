   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="9" CULP="-0.1" CH="Beispiel: &#x5;Startwert: Kochen: + (12)&#x5;1. Strich: Kochen: + (12)&#x5;2. Strich: Kochen: + (13)&#x5;...&#x5;6. Strich: Kochen: ++ (15)&#x5;9. Strich: Kochen: ++ (16)&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Vn Nimbus Mono L Regular" CSHX="5" CSHY="-5" CSTROKE="Black" />
